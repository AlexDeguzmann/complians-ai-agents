{
  "version": 2,
  "name": "recruitment-system",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "index.js"
    },
    {
      "src": "/webhook",
      "dest": "index.js"
    },
    {
      "src": "/zebraagent-trigger",
      "dest": "index.js"
    },
    {
      "src": "/lionagent-trigger",
      "dest": "index.js"
    },
    {
      "src": "/whaleagent-trigger",
      "dest": "index.js"
    },
    {
      "src": "/vapi-callback",
      "dest": "index.js"
    },
    {
      "src": "/whaleagent-callback",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}
